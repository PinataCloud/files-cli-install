---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<!-- HTML Meta Tags -->
    <title>Pinata CLI</title>
    <meta name="description" content="The power of the Pinata API in your terminal">

    <!-- Facebook Meta Tags -->
    <meta property="og:url" content="https://cli.pinata.cloud">
    <meta property="og:type" content="website">
    <meta property="og:title" content="Pinata CLI">
    <meta property="og:description" content="The power of the Pinata API in your terminal">
    <meta property="og:image" content="https://dweb.mypinata.cloud/ipfs/QmNcdx9t48z7RQUXUZZHmuc4zBfyBxKLjDfEgmfhiop7j7?img-format=webp">

    <!-- Twitter Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta property="twitter:domain" content="cli.pinata.cloud">
    <meta property="twitter:url" content="https://cli.pinata.cloud">
    <meta name="twitter:title" content="Pinata CLI">
    <meta name="twitter:description" content="The power of the Pinata API in your terminal">
    <meta name="twitter:image" content="https://dweb.mypinata.cloud/ipfs/QmNcdx9t48z7RQUXUZZHmuc4zBfyBxKLjDfEgmfhiop7j7?img-format=webp">
		<meta name="generator" content={Astro.generator} />
	</head>
	<body>
		<slot />
	</body>
</html>
